/*
Question Link :- https://www.naukri.com/code360/problems/binary-to-integer_2421938
*/


#include <bits/stdc++.h>

int binaryToInteger(int n, Node *head) {
    int ans = 0;

    while (n--) {
        int temp = head -> data;
        head = head -> next;

        if (temp == 1) {
            ans += pow(2, n);
        }
    }
    return ans;
}
