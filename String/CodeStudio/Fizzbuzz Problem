/*
Question Link :- https://www.naukri.com/code360/problems/hello-world_7118522
*/


vector<string> helloWorld(int N) {
    vector<string>ans;

    for (int i = 1; i <= N; i++) {
        string s;
        if (i % 3 == 0) {
            s += "Hello";
        }
        if (i % 5 == 0) {
            s += "World";
        }
        if (s.empty()) {
            ans.push_back(to_string(i));
        }
        else {
            ans.push_back(s);
        }
    }
    return ans;
}
